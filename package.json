{
    "name": "pokemon-gen5-initseed",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "dev:agent": "cross-env VITE_ENABLE_VERBOSE_VERIFICATION=false vite",
        "dev:full": "npm run build:wasm && node scripts/copy-wasm-files.js && npm run dev",
        "build": "npm run build:wasm && node scripts/copy-wasm-files.js && npm run build:ts",
        "build:wasm": "node scripts/build-wasm-optimized.js",
        "build:wasm:debug": "cd wasm-pkg && wasm-pack build --target web --out-dir pkg --dev",
        "build:wasm:size": "cd wasm-pkg && wasm-pack build --target web --out-dir pkg --release --profile release-size",
        "build:ts": "tsc -b --noCheck && vite build",
        "deploy": "npm run deploy:build && npm run deploy:copy",
        "deploy:build": "cross-env NODE_ENV=production npm run build",
        "deploy:copy": "rimraf docs && copyfiles -u 1 \"dist/**/*\" docs",
        "clean": "rimraf dist wasm-pkg/pkg wasm-pkg/target src/wasm public/wasm",
        "lint": "eslint .",
        "preview": "vite preview",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:ui": "vitest --ui",
        "test:rust": "cd wasm-pkg && cargo test",
        "test:rust:browser": "cd wasm-pkg && wasm-pack test --chrome --headless",
        "test:all": "npm run test:rust && npm run test:rust:browser && npm run test",
        "test:e2e": "node scripts/run-e2e-consistency-test.js",
        "test:performance": "node scripts/performance-comparison.js",
        "copy:wasm": "node scripts/copy-wasm-files.js",
        "scrape:encounters": "node scripts/scrape-encounters.js"
    },
    "dependencies": {
        "@heroicons/react": "^2.2.0",
        "@hookform/resolvers": "^5.2.1",
        "@octokit/core": "^7.0.3",
        "@phosphor-icons/react": "^2.1.7",
        "@radix-ui/colors": "^3.0.0",
        "@radix-ui/react-accordion": "^1.2.3",
        "@radix-ui/react-alert-dialog": "^1.1.6",
        "@radix-ui/react-aspect-ratio": "^1.1.2",
        "@radix-ui/react-avatar": "^1.1.3",
        "@radix-ui/react-checkbox": "^1.1.4",
        "@radix-ui/react-collapsible": "^1.1.3",
        "@radix-ui/react-context-menu": "^2.2.6",
        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-dropdown-menu": "^2.1.6",
        "@radix-ui/react-hover-card": "^1.1.6",
