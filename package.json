        "@types/react": "^19.0.10",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "@vitejs/plugin-react-swc": "^4.0.0",
        "@vitest/ui": "^3.2.4",
        "cheerio": "^1.0.0-rc.12",
        "copyfiles": "^2.4.1",
        "cross-env": "^7.0.3",
        "eslint": "^9.28.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "happy-dom": "^18.0.1",
        "node-fetch": "^3.3.2",
        "rimraf": "^6.0.1",
        "tailwindcss": "^4.0.17",
        "typescript": "~5.7.2",
        "typescript-eslint": "^8.39.0",
