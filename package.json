        "date-fns": "^3.6.0",
        "embla-carousel-react": "^8.5.2",
        "framer-motion": "^12.6.2",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.537.0",
        "marked": "^15.0.7",
        "next-themes": "^0.4.6",
        "octokit": "^5.0.3",
        "react": "^19.0.0",
        "react-day-picker": "^9.8.1",
        "react-dom": "^19.0.0",
        "react-error-boundary": "^6.0.0",
        "react-hook-form": "^7.54.2",
        "react-resizable-panels": "^2.1.7",
        "recharts": "^2.15.1",
        "sonner": "^2.0.7",
